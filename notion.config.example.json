{
  "databases": [
    {
      "databaseId": "your-notion-database-id-here",
      "srcDir": "src/posts",
      "srcDirImages": "src/assets/images/posts",
      "basePath": "/blog",
      "layout": "layouts/post.njk",
      "cleanBeforeSync": true,
      "excludeProperties": ["Internal Notes", "Draft"],
      "slug": {
        "from": "title",
        "fallback": "id",
        "lower": true
      },
      "permalink": "/blog/{slug}/",
      "frontMatter": {
        "tags": "post",
        "templateEngineOverride": "njk,md"
      }
    },
    {
      "databaseId": "another-database-id",
      "srcDir": "src/projects",
      "srcDirImages": "src/assets/images/projects",
      "basePath": "/projects",
      "layout": "layouts/project.njk",
      "permalink": "/projects/{slug}/"
    },
    {
      "databaseId": "pages-database-id",
      "srcDir": "src/pages",
      "srcDirImages": "src/assets/images/pages",
      "basePath": "/pages",
      "layout": "layouts/page.njk",
      "excludeProperties": ["Status"],
      "frontMatter": {
        "eleventyExcludeFromCollections": false
      }
    }
  ]
}
